<div class="page-header">
  <%= link_to checkins_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_checkin_path(@checkin), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show checkin</h1>
</div>

<dl class="dl-horizontal">
  <dt>Latitude:</dt>
  <dd><%= @checkin.latitude %></dd>

  <dt>Longitude:</dt>
  <dd><%= @checkin.longitude %></dd>

  <dt>Name:</dt>
  <dd><%= @checkin.name %></dd>

  <dt>Address:</dt>
  <dd><%= @checkin.address %></dd>

  <dt>Start:</dt>
  <dd><%= @checkin.start %></dd>

  <dt>User:</dt>
  <dd><%= @checkin.user %></dd>

</dl>
